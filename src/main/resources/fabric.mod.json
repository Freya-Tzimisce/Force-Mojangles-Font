{
  "schemaVersion": 1,
  "id": "forcemojanglesfont",
  "version": "${version}",
  "name": "Force Mojangles Font",
  "description": "Before Minecraft 1.13, if the language file of the corresponding language contains non-ASCII characters, all charactors will be rendered as unicode font style.\nThis mod will solve this issue.",
  "authors": [
    "ZekerZhayard",
    "Freya Tzimisce"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/forceasciifont",
    "sources": "https://github.com/Freya-Tzimisce/Force-Mojangles-Font/"
  },
  "license": "GPLv3",
  "icon": "assets/forcemojanglesfont/icon.png",
  "environment": "client",
  "entrypoints": {},
  "mixins": [
    "forcemojanglesfont.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": [
      ">=1.6",
      "<=1.12.2"
    ]
  },
  "suggests": {}
}
